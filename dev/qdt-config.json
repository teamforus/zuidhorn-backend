                            "angular/directives/*.js",
                            "angular/providers/*.js",
                            "angular/services/*.js",
                            "angular/filters/*.js",
                            "app.js"
                        ],
                        "dest": "/",
                        "name": "app.js"
                    }, {
                        "path": "/raw",
                        "src": "raw/**/*.js",
                        "dest": "/"
                    }]
                }
            },
            "server": {
                "path": "/",
                "port": 3000
            }
        },
