                "to": "assets/dist/file-saver"
            }, {
                "from": "../../node_modules/jshashes/hashes.min.js",
                "to": "assets/dist/jshashes"
            }],
            "server": false,
            "tasks": {
                "disabled": {
                    "pug": false,
                    "ts": true,
                    "js": false,
                    "assets": false
                },
                "settings": {
                    "less": [],
                    "js": [{
                        "src": [
                            "angular/angular.app.js",
                            "angular/controllers/**/**.js",
                            "angular/components/**/**.js",
                            "angular/directives/**/**.js",
                            "angular/services/**/**.js",
                            "angular/filters/**/**.js",
                            "app.js"
                        ],
                        "dest": "/",
                        "name": "app.js"
                    }, {
                        "path": "/raw",
                        "src": "raw/**/*.js",
                        "dest": "/"
                    }],
                    "scss": [{
                        "src": "style.scss",
                        "watch": "includes/**/*.scss",
                        "dest": "/",
                        "name": "style.min.css"
                    }],
                    "pug": [{
                        "path": "/",
                        "src": ["*.pug"],
                        "watch": ["layout/**/*.pug"],
                        "dest": "/"
                    }, {
                        "path": "/raw/tpl",
                        "src": ["raw/**/*.pug"],
                        "dest": "/tpl"
                    }]
                }
            }
        },
